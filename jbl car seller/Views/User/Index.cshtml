@model IEnumerable<User>

@{
    Layout = "_AdminLayout";
    ViewData["Title"] = "User List";
}

<div class="container">
    <div class="row">
        <div class="col-8 text-center">
            <h2>@ViewData["Title"]</h2>
        </div>
      
    </div>


    <!-- Table to display car information -->
    <table class="table table-striped table-bordered rounded-3 text-center">
        <thead class="table-dark">
            <tr>
                
                <th>Id</th>

                <th>Name</th>
                <th>Email</th>

                <th>Password</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.FullName</td>

                    <td>@user.Email</td>
                    <td>@user.PasswordHash</td> 

                   
                </tr>
            }
        </tbody>
    </table>



</div>

